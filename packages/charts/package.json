        "@react-candles/core": "file:../core",
        "@react-candles/indicators": "file:../indicators",
        "@react-candles/interactive": "file:../interactive",
        "@react-candles/scales": "file:../scales",
        "@react-candles/series": "file:../series",
        "@react-candles/tooltip": "file:../tooltip",
        "@react-candles/utils": "file:../utils"
    },
    "peerDependencies": {
        "react": "^16.0.0 || ^17.0.0 || ^18.0.0",
        "react-dom": "^16.0.0 || ^17.0.0 || ^18.0.0"
