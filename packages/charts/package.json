{
    "name": "react-candles",
    "version": "2.0.1",
    "description": "React charts specific to finance.",
    "main": "./lib/index.js",
    "typings": "./lib/index.d.ts",
    "files": [
        "lib",
        "src"
    ],
    "sideEffects": false,
    "author": "reactivemarkets",
    "keywords": [
        "charts",
        "charting",
        "stockcharts",
        "finance",
        "financial",
        "finance-chart",
        "react",
        "d3"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/reactivemarkets/react-candles.git"
    },
    "bugs": {
        "url": "https://github.com/reactivemarkets/react-candles/issues"
    },
    "scripts": {
        "build": "npm run clean && npm run compile",
